<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<connectionStrings>
		<add name="myConn"
    connectionString="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=GameLand;Integrated Security=True"
    providerName="System.Data.SqlClient" />
	</connectionStrings>
 <system.serviceModel>
  <bindings>
   <basicHttpBinding>
    <binding name="GameServiceSoap">
     <security mode="Transport" />
    </binding>
    <binding name="GameServiceSoap1" />
    <binding name="GameServiceSoap2">
     <security mode="Transport" />
    </binding>
    <binding name="GameServiceSoap3" />
   </basicHttpBinding>
  </bindings>
  <client>
   <endpoint address="https://localhost:44378/GameService.asmx"
    binding="basicHttpBinding" bindingConfiguration="GameServiceSoap"
    contract="GameServiceRef.GameServiceSoap" name="GameServiceSoap" />
   <endpoint address="https://localhost:44378/GameService.asmx"
    binding="basicHttpBinding" bindingConfiguration="GameServiceSoap2"
    contract="GameLandWebServiceRef.GameServiceSoap" name="GameServiceSoap1" />
  </client>
 </system.serviceModel>
</configuration>